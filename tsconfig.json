{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": ["geojson"] // Add this line
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/pages/dashPat/appointemente/page.jsx"
, "app/api/books/[bookid]/route.js", "app/api/books/[bookid]/route.js", "app/catalog/[id]/page.jsx", "app/api/books/[bookid]/route.js", "app/api/books/[bookid]/route.js", "app/api/auth/[...nextauth].js", "app/utils/ graph.ts", "app/utils/astar.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
